{
  "kind": "collectionType",
  "collectionName": "carts",
  "info": {
    "singularName": "cart",
    "pluralName": "carts",
    "displayName": "cart",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "externalUserId": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 255
    },
    "userEmail": {
      "type": "email",
      "required": true
    },
    "totalAmount": {
      "type": "decimal",
      "default": 0
    },
    "currency": {
      "type": "string",
      "default": "RS",
      "maxLength": 10
    },
    "lastUpdated": {
      "type": "datetime"
    },
    "cart_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-item.cart-item",
      "mappedBy": "cart"
    }
  }
}
