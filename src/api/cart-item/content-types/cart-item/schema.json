{
  "kind": "collectionType",
  "collectionName": "cart_items",
  "info": {
    "singularName": "cart-item",
    "pluralName": "cart-items",
    "displayName": "cart-item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "unitPrice": {
      "type": "decimal",
      "required": true
    },
    "totalPrice": {
      "type": "decimal",
      "required": true
    },
    "productVariant": {
      "type": "string",
      "maxLength": 255
    },
    "cart": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::cart.cart",
      "inversedBy": "cart_items"
    },
    "productSlug": {
      "type": "string",
      "required": true
    }
  }
}
